<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jhonnymichel.pybr14" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Python Brasil 2018</name>
    <description>
        Explore a programação do evento com o app oficial da Python Brasil 14.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <splash src="res/screen/ios/Default@2x~universal~anyany.png" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#102E46" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <preference name="BackgroundColor" value="0x102E46" />
</widget>
