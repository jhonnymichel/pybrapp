<?xml version='1.0' encoding='utf-8'?>
<widget id="org.pythonbrasil.pybr2018" version="1.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Python Brasil 2018</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="jhonnymichel@evolux.net.br" href="https://2018.pythonbrasil.org.br">
        Evolux
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarBackgroundColor" value="#0D273C" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <preference name="BackgroundColor" value="0x102E46" />
</widget>
